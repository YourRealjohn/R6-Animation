-- OPEN SOURCE
-- Universal R6 animation
loadstring(game:HttpGet("https://raw.githubusercontent.com/YourRealjohn/R6-Animation/refs/heads/main/R6-ANIMATION"))()

-- Countdown before executing the emote script
for i = 6, 1, -1 do
    game.StarterGui:SetCore("SendNotification", {
        Title = "Countdown",
        Text = "Executing in " .. i .. " second(s)",
        Duration = 1
    })
    task.wait(1)
end

-- Final message before execution
game.StarterGui:SetCore("SendNotification", {
    Title = "Good luck",
    Text = "Enjoy the script now buddy",
    Duration = 5
})

task.wait(1)

-- The Emote
loadstring(game:HttpGet("https://raw.githubusercontent.com/YourRealjohn/R6-Animation/refs/heads/main/The3Emote"))()
